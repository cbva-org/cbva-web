[{"key":"prize-pool-1","content":{"root":{"type":"root","format":"","indent":0,"version":1,"children":[{"type":"paragraph","format":"","indent":0,"version":1,"children":[{"mode":"normal","text":"Prize money for Open division tournaments is funded directly by player registrations.","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null,"textStyle":"","textFormat":0},{"type":"paragraph","format":"","indent":0,"version":1,"children":[{"type":"linebreak","version":1},{"mode":"normal","text":"From every team that enters an Open division event, $40 of the registration fee is allocated to the prize pool. These funds are combined to create the total payout for that tournament and are awarded to top finishers.","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null,"textStyle":"","textFormat":0},{"type":"paragraph","format":"","indent":0,"version":1,"children":[{"type":"linebreak","version":1},{"mode":"normal","text":"This structure ensures that prize money scales with participation. Larger Open draws result in larger prize pools, while keeping the process simple and transparent for players.","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null,"textStyle":"","textFormat":0},{"type":"paragraph","format":"","indent":0,"version":1,"children":[{"type":"linebreak","version":1},{"mode":"normal","text":"Prize payouts and placement breakdowns can be found below.","type":"text","style":"","detail":0,"format":0,"version":1},{"type":"linebreak","version":1}],"direction":null,"textStyle":"","textFormat":0}],"direction":null}},"page":"prize-pool"},{"key":"prize-pool-2","content":{"root":{"type":"root","format":"","indent":0,"version":1,"children":[{"type":"paragraph","format":"","indent":0,"version":1,"children":[{"mode":"normal","text":"Premier events feature a fixed prize payout.","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null,"textStyle":"","textFormat":0},{"type":"paragraph","format":"","indent":0,"version":1,"children":[{"mode":"normal","text":"Prize money is awarded as follows:","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null,"textStyle":"","textFormat":0},{"tag":"ul","type":"list","start":1,"format":"","indent":0,"version":1,"children":[{"type":"listitem","value":1,"format":"","indent":0,"version":1,"children":[{"mode":"normal","text":"1st Place: $1,250","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null},{"type":"listitem","value":2,"format":"","indent":0,"version":1,"children":[{"mode":"normal","text":"2nd Place: $750","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null},{"type":"listitem","value":3,"format":"","indent":0,"version":1,"children":[{"mode":"normal","text":"3rd Place (each team): $250","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null}],"listType":"bullet","direction":null},{"type":"paragraph","format":"","indent":0,"version":1,"children":[{"type":"linebreak","version":1},{"mode":"normal","text":"Prize payouts are the same for every Premier event and are distributed to teams based on finish.","type":"text","style":"","detail":0,"format":0,"version":1}],"direction":null,"textStyle":"","textFormat":0}],"direction":null}},"page":"prize-pool"}]